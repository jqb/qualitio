NameVirtualHost *:8081
Listen 8081

<VirtualHost *:8081>
    ServerAdmin webmaster@localhost

    WSGIScriptAlias / ${PATH}/deploy/qualitio.wsgi

    Alias /static/ ${PATH}/qualitio/qualitio/static/
    <Directory ${PATH}/qualitio/qualitio/static/>
        Order deny,allow
        Allow from all
    </Directory>

    ErrorLog ${APACHE_LOG_DIR}/qualitio-error.log
    LogLevel warn
    CustomLog ${APACHE_LOG_DIR}/qualitio-access.log combined
    
</VirtualHost>
